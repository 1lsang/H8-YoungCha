{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build",
    "prettier": "prettier --write --cache .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
